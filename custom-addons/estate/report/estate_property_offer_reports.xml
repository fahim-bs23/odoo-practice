<?xml version="1.0"?>
<odoo>
        <data>
            <record id="report_property_id_offers" model="ir.actions.report">
                <field name="name">Property Offers</field>
                <field name="model">estate.property</field>
                <field name="report_type">qweb-pdf</field>
                <field name="report_name">estate.estate_property_report</field>
                <field name="report_file">estate.estate_property_report</field>
                <field name="binding_model_id" ref="model_estate_property"/>
                <field name="binding_type">report</field>
            </record>
        </data>

        <data>
            <record id="action_report_estate_salesman" model="ir.actions.report">
                    <field name="name">Salesman Property Offers</field>
                    <field name="model">res.users</field>
                    <field name="report_type">qweb-pdf</field>
                    <field name="report_name">estate.report_salesman_properties</field>
                    <field name="report_file">estate.report_salesman_properties</field>
                    <field name="print_report_name">'Salesman Property Offers'</field>
                    <field name="binding_model_id" ref="base.model_res_users"/>
            </record>
        </data>

        <record id="estate_property_xlsx" model="ir.actions.report">
            <field name="name">Estate Property Excel</field>
            <field name="model">estate.property</field>
            <field name="report_type">xlsx</field>
            <field name="report_name">estate.report_estate_property_xls</field>
            <field name="report_file">estate.report_estate_property_xls</field>
            <field name="binding_model_id" ref="model_estate_property"/>
        </record>
</odoo>